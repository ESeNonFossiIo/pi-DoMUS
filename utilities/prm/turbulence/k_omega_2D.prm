  set Non linear term                      = linear

  # Parameters:
  set nu [Pa s]                            = 1e-5
  set rho [kg m^3]                         = 1.0

  set alpha                                = 0.5555555556
  set beta                                 = 0.075
  set beta_star                            = 0.09
  set sigma                                = .5
  set sigma_star                           = .6

  set SUPG alpha                           = 1.0
  set div-grad stabilization parameter     = 0.9 #0.09999
end

