
subsection ALE Navier Stokes Interface
  set AMG d - use inverse operator     = true
  set AMG u - use inverse operator     = true
  set AMG v - use inverse operator     = true
  set Block of differential components = 1,1,1,0
  set Blocking of the finite element   = d,d,v,v,u,u,p
  set Finite element space             = FESystem[FE_Q(2)^d-FE_Q(2)^d-FE_Q(2)^d-FE_Q(1)]
  set Invert Mp using inverse operator = false
  set Use mass matrix for d_dot        = true
  set c                                = 0.0
  set k                                = 1.0
  set nu [Pa s]                        = 10
  set rho [Kg m^-d]                    = 1.0
end

subsection Dirichlet boundary conditions
  set IDs and component masks = 2=d % 3=d
  set IDs and expressions     = 2=0; a*((x-0.5)^2)*sin(t*v);0;0;0;0;0; %\
                                3=0;-a*((x-0.5)^2)*sin(t*v);0;0;0;0;0;
  set Known component names   = d,d,v,v,u,u,p
  set Used constants          = a=0.1, v=16
end
subsection Time derivative of Dirichlet boundary conditions
  set IDs and component masks = 2=d % 3=d
  set IDs and expressions     = 2=0; a*((x-0.5)^2)*v*cos(t*v);0;0;0;0;0; %\
                                3=0;-a*((x-0.5)^2)*v*cos(t*v);0;0;0;0;0;
  set Known component names   = d,d,v,v,u,u,p
  set Used constants          = a=0.1, v=16
end
subsection Nitsche boundary conditions
  set IDs and component masks = 2=u % 3=u
